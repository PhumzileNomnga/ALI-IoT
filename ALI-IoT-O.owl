        <rdfs:domain rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#User"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Activity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#hasSubActivity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#hasSubActivity">
