        <rdfs:domain rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Device"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Sensor"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#User"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#involvesDevice -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#involvesDevice">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Activity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Device"/>
        <rdfs:comment>to indicate that an activity involve a particular device e.g. temperatureReading involvesDevice smartThermostat</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#isDetectedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#isDetectedBy">
