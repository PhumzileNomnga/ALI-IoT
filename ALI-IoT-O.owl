    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#generatedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#generatedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Event"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Sensor"/>
        <rdfs:comment>dmonstrates that a particular event is generated by a specific sensor e.g. eventA generatedBy sensorA</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#hasActivity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#hasActivity">
