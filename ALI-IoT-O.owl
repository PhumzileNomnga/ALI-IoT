        <rdfs:range rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Sensor"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#isOperatedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#isOperatedBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Sensor"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#User"/>
        <rdfs:comment>Demonstrates which user operates or is responsible for a sensor e.g. MotionDetector isOperatedBy UserA</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#isPartOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Device"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Sensor"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Device"/>
        <rdfs:comment>Representing that a sensor is part of larger device e.g sensorA isPartOf deviceA</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#monitorsEvent -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#monitorsEvent">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Sensor"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Event"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#performedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#performedBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Activity"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#User"/>
        <rdfs:comment>to indicate that an activity is performed by a specific sensor or user e.g. motionDetection performedBy motionSensor or sleeping performedBy userA</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#records -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#records">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Sensor"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Event"/>
        <rdfs:comment>to demonstrate that a sensor recods a specific event e.g sensorA records eventA</rdfs:comment>
    </owl:ObjectProperty>
    


