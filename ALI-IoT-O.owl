        <rdfs:range rdf:resource="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#Sub-Activity"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#hasUser -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pnomnga/ontologies/2020/3/ALI-IoT-Ontology#hasUser">
